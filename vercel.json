{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    },
    {
      "src": "*.html",
      "use": "@vercel/static"
    },
    {
      "src": "*.js",
      "use": "@vercel/static"
    },
    {
      "src": "*.css",
      "use": "@vercel/static"
    },
    {
      "src": "assets/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "server.js"
    },
    {
      "src": "/brain-grain-one.html",
      "dest": "/brain-grain-one.html"
    },
    {
      "src": "/Brain%20Grain%20Journey.html",
      "dest": "/Brain Grain Journey.html"
    },
    {
      "src": "/Brain%20Grain%20Platform%20\\(2\\).html",
      "dest": "/Brain Grain Platform (2).html"
    },
    {
      "src": "/Impact%20Dashboard.html",
      "dest": "/Impact Dashboard.html"
    },
    {
      "src": "/(.*\\.(js|css|png|jpg|jpeg|gif|svg|ico|html))",
      "dest": "/$1"
    },
    {
      "src": "/",
      "dest": "/brain-grain-one.html"
    },
    {
      "src": "/(.*)",
      "dest": "/brain-grain-one.html"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
